			"default": ""
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"socialLinks": {
			"type": "array",
			"default": [
				{
					"link": "https:/facebook.com",
					"icon": "facebook"
				},
				{
					"link": "https:/google.com",
					"icon": "google"
				}
			]
		}
	},
	"textdomain": "gt_block",
	"editorScript": "file:../../../build/js/about-us/about-us.js",
	"editorStyle": "file:../../../build/css/about-us.css",
	"style": "file:../../../build/css/style-about-us.css"
}
