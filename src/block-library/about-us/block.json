{
	"apiVersion": 2,
	"name": "gt-block/about-us",
	"version": "0.1.0",
	"title": "About Us",
	"category": "text",
	"icon": "admin-users",
	"description": "About us widget for theme",
	"supports": {
		"html": false
	},
	"attributes": {
		"details": {
			"type": "string",
			"source": "html",
			"selector": "p"
